<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1000.0" prefWidth="1700.0" stylesheets="@css/BaseStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.u.controllers.NewRequestController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox fillHeight="false" prefHeight="800.0" prefWidth="1700.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="800.0" prefWidth="850.0" spacing="40.0">
                     <children>
                        <Label prefHeight="88.0" prefWidth="361.0" styleClass="headline-1" text="New Request" textAlignment="CENTER">
                           <font>
                              <Font size="60.0" />
                           </font>
                        </Label>
                        <Pane prefHeight="125.0" prefWidth="850.0" />
                        <JFXTextField fx:id="makeTitleField" labelFloat="true" maxWidth="-Infinity" prefHeight="50.0" prefWidth="600.0" promptText="Title">
                           <font>
                              <Font size="18.0" />
                           </font></JFXTextField>
                        <JFXTextArea fx:id="makeDescriptionField" focusColor="BLACK" labelFloat="true" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="170.0" prefWidth="600.0" promptText="Description">
                           <font>
                              <Font size="15.0" />
                           </font></JFXTextArea>
                        <JFXDatePicker fx:id="makeDate2BCompleteDatePicker" prefHeight="40.0" prefWidth="400.0" promptText="Date to Completed By">
