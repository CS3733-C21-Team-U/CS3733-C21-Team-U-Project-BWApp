<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.u.controllers.login.SettingsPageController">
   <children>
      <VBox layoutX="300.0" layoutY="134.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Settings Page" />
            <Label text="Path Finding Modes" />
            <HBox prefHeight="30.0" prefWidth="600.0">
               <children>
                  <JFXButton fx:id="pathMode1Button" onAction="#handlePathMode1" text="Mode 1" />
                  <JFXButton fx:id="pathMode2Button" onAction="#handlePathMode2" text="Mode 2" />
               </children>
            </HBox>
            <JFXButton fx:id="loadCSVButton" onAction="#handleLoadCSV" text="Load Saved CSV" />
            <Label text="Set theme" />
            <HBox prefHeight="33.0" prefWidth="600.0">
               <children>
                  <JFXButton fx:id="themeMode1Button" onAction="#handleThemeMode1" text="Theme 1" />
                  <JFXButton fx:id="themeMode2Button" onAction="#handleThemeMode2" text="Theme 2" />
               </children>
            </HBox>
            <Label text="Set Contact info" />
            <JFXTextField fx:id="phoneNumTextField" promptText="Phone #" />
            <JFXTextField fx:id="emailAddressTextField" promptText="E-mail Address" />
            <JFXButton fx:id="contactChangeButton" onAction="#handleContactChange" text="Apply Contact Change" />
            <Label fx:id="check" />
         </children>
      </VBox>
   </children>
</AnchorPane>
